{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "mocha": true
  },
  "rules": {
    "require-jsdoc": 0,
    "max-len": [1, 120, 2, {"ignoreComments": true}]
  },
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:react/recommended"
  ]
}